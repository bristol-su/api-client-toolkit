{
  "name": "bristol-su/api-client-toolkit",
  "description": "A set of interfaces and tools for adding to the PHP Api Client for the portal",
  "license": "MIT",
  "authors": [
    {
      "name": "Toby Twigger",
      "email": "tobytwigger1@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "psr/http-client": "^1.0",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "phpspec/prophecy-phpunit": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "BristolSU\\ApiToolkit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BristolSU\\Tests\\ApiToolkit\\": "tests/"
    }
  }
}
